{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Default": ""
  },
  "Quartz": {
    "CsvReaderFakeJob": {
      "Schedule": "0/5 * * * * ?",
      "Group": "my tasks",
      "Description": "This job does .CSV file load processing",
      "Instances": 1
    },
    "AluraJob": {
      "Schedule": "0/5 * * * * ?",
      "Group": "my tasks",
      "Description": "This job does...",
      "Instances": 1
    }
  },
  "HealthCheckApp": {
    "Enable": true,
    "AccessUrls": "https://localhost:3001;http://localhost:3000"
  },
  "HealthChecksUI": {
    "EvaluationTimeInSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}